<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/contenedorPrincipal"
    tools:context="com.example.kellyjohanazapataestrada.practica5.MainActivity">
    <FrameLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/contenedorFrame"
        tools:context="com.example.kellyjohanazapataestrada.practica5.LogginActivity">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/fondomain2"
            android:layout_weight="2.22">
        </LinearLayout>
    </FrameLayout>
    <ListView
        android:layout_width="300dp"
        android:layout_height="match_parent"
        android:id="@+id/menuIzqui"
        android:choiceMode="singleChoice"
        android:foregroundGravity="left"
        android:layout_gravity="start">

    </ListView>
</android.support.v4.widget.DrawerLayout>




